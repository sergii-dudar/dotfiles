        "status-icons": {
            "playing": "<span color='#c678dd'>ÔÖÑ </span>",
            "paused": "<span color='#98be65'>Ôäã </span>",
            "stopped": "<span color='#FC9867'>Ôäç </span>"
        },
        // "ignored-players": ["firefox"]
    },
    "custom/weather": {
        "format": "{}",
        "exec": "~/.config/waybar/module/shell/module.open-weather",
        "return-type": "text",
        "on-click": "brave https://sinoptik.ua/pohoda/vinnytsia",
        "on-click-right": "gnome-weather",
        "interval": 60
    },
